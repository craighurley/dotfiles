# custom scripts
[[ -d "$HOME/.scripts" ]] && PATH="$PATH:$HOME/.scripts"

# rgrav
[[ -d "$HOME/Projects/rgrav" ]] && PATH="$PATH:$HOME/Projects/rgrav"

# ruby
[[ -d "$HOME/.rbenv/bin" ]] && PATH="$HOME/.rbenv/bin:$PATH"

# oracle sqlplus
[[ -d "/Library/Oracle/instantclient" ]] && PATH="/Library/Oracle/instantclient:$PATH"

export PATH
