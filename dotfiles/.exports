# history
if [[ $(echo $SHELL | grep zsh) ]] ; then
    export HISTFILE=~/.zsh_history
    export HISTTIMEFORMAT="[%F %T] "
elif [[ $(echo $SHELL | grep bash) ]] ; then
    export HISTFILE=~/.bash_history
    export HISTTIMEFORMAT="%Y-%m-%d %T "
fi
export HISTSIZE=10000
export HISTFILESIZE=$HISTSIZE
export HISTCONTROL=ignoreboth:erasedups
export HISTIGNORE="clear:c:history:h:exit:e"
export HISTORY_IGNORE="clear|c|history|h|exit|e"

# docker images
export DOCKER_CLI_HINTS=false
export DOCKER_AWS_CLI_VERSION=latest
export DOCKER_ORG_FORMATION_CLI_VERSION=latest
export DOCKER_SCEPTRE_VERSION=latest

# editor
export EDITOR=vim

# golang
# avoid issues with m1/rosetta
#export GODEBUG=asyncpreemptoff=1
export GOPATH=$HOME/go

# # gpg
# export GPG_TTY=$(tty)
# export GPG_AGENT_INFO=$HOME/.gnupg/S.gpg-agent

# grep defaults
export GREP_OPTIONS="--color -I --exclude-dir=.git --exclude-dir=.svn --exclude-dir=.metadata --exclude=.DS_Store"

# homebrew options
export HOMEBREW_CASK_OPTS="--require-sha"
export HOMEBREW_NO_ANALYTICS=1
export HOMEBREW_NO_EMOJI=1
export HOMEBREW_NO_ENV_HINTS=1

# Don't clear the screen after quitting a manual page
export MANPAGER="less -X"

# # openssl
# if [[ -d $(brew --prefix)/opt/openssl/bin ]] ; then
#     export LDFLAGS="-L$(brew --prefix)/opt/openssl/lib"
#     export CPPFLAGS="-I$(brew --prefix)/opt/openssl/include"
# fi

# # sqldeveloper
# if [[ -d $HOME/Applications/Oracle/instaclient ]] ; then
#     export DYLD_LIBRARY_PATH=$HOME/Applications/Oracle/instaclient
# fi

# python
export PYENV_VIRTUALENV_DISABLE_PROMPT=1

# steampipe
export STEAMPIPE_TELEMETRY=none
export STEAMPIPE_UPDATE_CHECK=false

# Use UTF-8
#export LANG="en_NZ"
#export LC_ALL="en_NZ.UTF-8"

# terraform
export TFENV_AUTO_INSTALL=false

