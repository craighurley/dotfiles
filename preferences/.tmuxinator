#!/usr/bin/env bash

[[ ! -d ~/.tmuxinator ]] && mkdir -p ~/.tmuxinator
chmod 744 ~/.tmuxinator

if [[ ! -f ~/.tmuxinator/aws.yaml ]] ; then
    cat << EOF > ~/.tmuxinator/aws.yaml
name: aws

startup_window: aws

windows:
  - general:
  - aws:
    layout: main-vertical
    panes:
      -
      -
  - git:
EOF
fi
